{
    "name": "service_member2",
    "title": "服务成员",
    "table": "service_member",
    "key": "member_id",
    "orderby_default": "`member_id` desc",
    "field_obj": "`member_id`,`user_id`,`way`,`type_id`,`city_id`,`area_id`,`position_x`,`position_y`,`price`,`service_phone`,`servicer_name`,`address`",
    "field_default": "`member_id`,`user_id`,`way`,`type_id`,`city_id`,`area_id`,`position_x`,`position_y`,`price`,`time_create`,`time_update`,`service_phone`,`servicer_name`,`address`",
    "query": {
        "way_min": "`way` >= {0}",
        "way_max": "`way` <= {0}",
        "position_x_min": "`position_x` >= {0}",
        "position_x_max": "`position_x` <= {0}",
        "position_y_min": "`position_y` >= {0}",
        "position_y_max": "`position_y` <= {0}",
        "price_min": "`price` >= {0}",
        "price_max": "`price` <= {0}",
        "time_create_min": "`time_create` >= '{0}'",
        "time_create_max": "`time_create` <= '{0}'",
        "time_update_min": "`time_update` >= '{0}'",
        "time_update_max": "`time_update` <= '{0}'",
        "service_phone": "`service_phone` like '%{0}%'",
        "servicer_name": "`servicer_name` like '%{0}%'",
        "address": "`address` like '%{0}%'",
        "keyword": "(`servicer_name` like '%{0}%')"
    },
    "update": {
        "way_add": "`way` = `way` + {0}",
        "position_x_add": "`position_x` = `position_x` + {0}",
        "position_y_add": "`position_y` = `position_y` + {0}",
        "price_add": "`price` = `price` + {0}"
    },
    "format": [
        {
            "key": "user_id",
            "title": "用户",
            "table": "user_account",
            "name": "nickname"
        },
        {
            "key": "way",
            "title": "收费方式",
            "list": [
                "",
                "次",
                "时",
                "日",
                "周",
                "月",
                "季",
                "年"
            ]
        },
        {
            "key": "type_id",
            "title": "默认分类",
            "table": "service_type",
            "name": "name"
        },
        {
            "key": "city_id",
            "title": "所属城市",
            "table": "sys_address_city",
            "name": "name"
        },
        {
            "key": "area_id",
            "title": "所属市区",
            "table": "sys_address_area",
            "name": "name"
        }
    ],
    "field_hide": []
}