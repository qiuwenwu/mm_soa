{
    "name": "dev_order2",
    "title": "订单信息",
    "table": "dev_order",
    "key": "order_id",
    "orderby_default": "`order_id` desc",
    "field_obj": "`order_id`,`orderid`,`name`,`price`,`doc`,`file`,`lang`,`mode`,`ui`,`db`,`framework`,`state`,`balance`,`tpl_home`,`tpl_admin`,`publisher`,`handler`,`time_end`,`dispose_log`,`config`",
    "field_default": "`order_id`,`orderid`,`name`,`price`,`doc`,`file`,`lang`,`mode`,`ui`,`db`,`framework`,`state`,`balance`,`tpl_home`,`tpl_admin`,`publisher`,`handler`,`time_create`,`time_update`,`time_end`,`dispose_log`",
    "query": {
        "orderid": "`orderid` like '%{0}%'",
        "name": "`name` like '%{0}%'",
        "time_end_min": "`time_end` >= '{0}'",
        "time_end_max": "`time_end` <= '{0}'",
        "lang": "`lang` like '%{0}%'",
        "state_min": "`state` >= {0}",
        "state_max": "`state` <= {0}",
        "tpl_home": "`tpl_home` like '%{0}%'",
        "tpl_admin": "`tpl_admin` like '%{0}%'",
        "publisher": "`publisher` like '%{0}%'",
        "handler": "`handler` like '%{0}%'",
        "keyword": "(`name` like '%{0}%')",
        "price_min": "`price` >= {0}",
        "price_max": "`price` <= {0}",
        "doc": "`doc` like '%{0}%'",
        "file": "`file` like '%{0}%'",
        "ui": "`ui` like '%{0}%'",
        "db": "`db` like '%{0}%'",
        "framework": "`framework` like '%{0}%'",
        "time_create_min": "`time_create` >= '{0}'",
        "time_create_max": "`time_create` <= '{0}'",
        "time_update_min": "`time_update` >= '{0}'",
        "time_update_max": "`time_update` <= '{0}'",
        "mode": "`mode` like '%{0}%'",
        "dispose_log": "`dispose_log` like '%{0}%'",
        "config": "`config` like '%{0}%'"
    },
    "update": {
        "state_add": "`state` = `state` + {0}",
        "price_add": "`price` = `price` + {0}"
    },
    "format": [
        {
            "key": "mode",
            "title": "模式",
            "list": [
                {
                    "name": "MVC",
                    "value": "MVC"
                },
                {
                    "name": "MVVM",
                    "value": "MVVM"
                }
            ]
        },
        {
            "key": "state",
            "title": "订单状态",
            "list": [
                "",
                "待取单",
                "待开发",
                "进行中",
                "已完成",
                "修改中",
                "已删除"
            ]
        },
        {
            "key": "balance",
            "title": "已结算",
            "list": [
                "否",
                "是"
            ]
        }
    ],
    "field_hide": []
}