        "area_min": "`area` >= {0}",
        "area_max": "`area` <= {0}",
        "phone": "`phone` like '%{0}%'",
        "name": "`name` like '%{0}%'",
        "house_number": "`house_number` like '%{0}%'",
        "address": "`address` like '%{0}%'",
        "keyword": "(`name` like '%{0}%')"
    },
    "query_default": {
        "user_id": "`user_id` = {user_id}"
    },
    "update": {
        "province_add": "`province` = `province` + {0}",
