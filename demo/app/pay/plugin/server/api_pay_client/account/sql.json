{
    "name": "pay_account",
    "title": "支付账户",
    "table": "pay_account",
    "key": "user_id",
    "orderby_default": "`user_id` desc",
    "field_obj": "`user_id`,`state`,`bank`,`bank_name`,`bank_state`,`wechat`,`wechat_state`,`alipay`,`alipay_state`,`btc`,`btc_state`,`eth`,`eth_state`,`eos`,`eos_state`,`mm`,`mm_state`",
    "field_default": "`user_id`,`state`,`bank`,`bank_name`,`bank_state`,`wechat`,`wechat_state`,`alipay`,`alipay_state`,`btc`,`btc_state`,`eth`,`eth_state`,`eos`,`eos_state`,`mm`,`mm_state`",
    "method": "get get_obj",
    "query": {
        "state_min": "`state` >= {0}",
        "state_max": "`state` <= {0}",
        "bank": "`bank` like '%{0}%'",
        "bank_name": "`bank_name` like '%{0}%'",
        "wechat": "`wechat` like '%{0}%'",
        "alipay": "`alipay` like '%{0}%'",
        "btc": "`btc` like '%{0}%'",
        "eth": "`eth` like '%{0}%'",
        "eos": "`eos` like '%{0}%'",
        "mm": "`mm` like '%{0}%'",
        "keyword": "(`bank_name` like '%{0}%')"
    },
    "query_default": {},
    "update": {
        "state_add": "`state` = `state` + {0}"
    },
    "format": [
        {
            "key": "state",
            "title": "状态",
            "list": [
                "",
                "正常",
                "异常",
                "冻结",
                "注销"
            ]
        },
        {
            "key": "bank_state",
            "title": "微信认证",
            "list": [
                "未认证",
                "已认证"
            ]
        },
        {
            "key": "wechat_state",
            "title": "微信认证",
            "list": [
                "未认证",
                "已认证"
            ]
        },
        {
            "key": "alipay_state",
            "title": "微信认证",
            "list": [
                "未认证",
                "已认证"
            ]
        },
        {
            "key": "btc_state",
            "title": "比特币地址认证",
            "list": [
                "未认证",
                "已认证"
            ]
        },
        {
            "key": "eth_state",
            "title": "以太币地址认证",
            "list": [
                "未认证",
                "已认证"
            ]
        },
        {
            "key": "eos_state",
            "title": "柚子币地址认证",
            "list": [
                "未认证",
                "已认证"
            ]
        },
        {
            "key": "mm_state",
            "title": "美眉币地址认证",
            "list": [
                "未认证",
                "已认证"
            ]
        }
    ]
}